{
  "dependencies": {
    "react-native-amap3d": "^2.0.0",
    "@react-native-community/geolocation": "^3.2.1",
    "react-native-permissions": "^4.1.5",
    "react-native-dotenv": "^3.4.11"
  },
  "devDependencies": {
    "@types/react-native-dotenv": "^0.2.2"
  },
  "notes": {
    "react-native-amap3d": "AMap (AutoNavi) React Native SDK for maps and location",
    "@react-native-community/geolocation": "Cross-platform geolocation API for React Native",
    "react-native-permissions": "Unified permissions API for React Native on iOS and Android",
    "react-native-dotenv": "Load environment variables from .env file",
    "@types/react-native-dotenv": "TypeScript definitions for react-native-dotenv"
  },
  "installation_commands": [
    "npm install react-native-amap3d @react-native-community/geolocation react-native-permissions react-native-dotenv",
    "npm install --save-dev @types/react-native-dotenv",
    "cd ios && pod install && cd ..",
    "npx react-native run-android",
    "npx react-native run-ios"
  ],
  "platform_specific_setup": {
    "android": {
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE", 
        "android.permission.ACCESS_WIFI_STATE",
        "android.permission.CHANGE_WIFI_STATE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_LOCATION_EXTRA_COMMANDS"
      ],
      "meta_data": "com.amap.api.v2.apikey"
    },
    "ios": {
      "info_plist_keys": [
        "NSLocationAlwaysAndWhenInUseUsageDescription",
        "NSLocationWhenInUseUsageDescription", 
        "NSLocationAlwaysUsageDescription"
      ],
      "app_delegate_import": "#import <AMapFoundationKit/AMapFoundationKit.h>"
    }
  }
}